{
  "name": "structure-playwrite",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test --reporter=line,allure-playwright",
    "test:ui": "npx playwright show-report",
    "test:debug": "PWDEBUG=1 npx playwright test",
    "test:record": "npx playwright codegen",
    "test:report": "npx allure generate allure-results --clean -o allure-report && npx allure open allure-report",
    "clean": "rm -rf node_modules package-lock.json",
    "install": "npm install",
    "depcheck": "npx depcheck",
    "build": "npm run clean && npm run install && npm run depcheck",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "allure-playwright": "^3.0.7",
    "depcheck": "^1.4.3",
    "prettier": "^3.4.2"
  },
  "dependencies": {
    "playwright": "^1.49.0"
  }
}
